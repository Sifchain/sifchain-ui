{
  "name": "sifchain-ui",
  "version": "0.1.2",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "bump": "cd ./app && yarn bump",
    "advance": "zx ./scripts/advance.mjs",
    "build": "VITE_APP_VERSION=$(git rev-parse HEAD) VITE_APP_SHA=$npm_package_version yarn turbo run build",
    "dev": "yarn turbo run dev --parallel",
    "e2e": "zx ./scripts/e2e.mjs",
    "lint": "zx ./scripts/lint.mjs",
    "postinstall": "patch-package && yarn workspace @sifchain/sdk run build && cd .. && husky install ./ui/.husky",
    "stack": "zx ./scripts/stack.mjs",
    "start": "zx ./scripts/start.mjs",
    "storybook": "zx ./scripts/storybook.mjs",
    "docs": "serve -p 4444 ../docs",
    "test": "zx ./scripts/test.mjs --tag feature__cosmos-0.42"
  },
  "devDependencies": {
    "arg": "^5.0.0",
    "concurrently": "^5.3.0",
    "ganache-cli": "^6.12.0",
    "husky": "^6.0.0",
    "patch-package": "^6.4.7",
    "pretty-quick": "^3.1.0",
    "serve": "^12.0.0",
    "truffle": "^5.1.48",
    "turbo": "^1.1.6",
    "untildify": "^4.0.0",
    "vercel": "^21.0.1",
    "wait-on": "^5.2.1",
    "wscat": "^4.0.1",
    "zx": "^4.3.0"
  },
  "workspaces": {
    "packages": [
      "core",
      "app",
      "e2e",
      "wallet-keplr",
      "wallet-terra-station",
      "wallet-metamask"
    ]
  },
  "resolutions": {
    "url-parse": "1.5.0"
  },
  "dependencies": {
    "@storybook/preset-scss": "^1.0.3",
    "@types/jsonp": "^0.2.0",
    "codecov": "^3.8.0",
    "css-loader": "^5.2.0",
    "jsonp": "^0.2.1",
    "prettier": "^2.2.1"
  }
}
