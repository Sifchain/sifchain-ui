{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": {
    "enabled": true
  },
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://sifchain.finance/legal-disclamer",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "US"
        }
      ],
      "permanent": false
    }
  ]
}
